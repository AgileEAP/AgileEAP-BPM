    </resourceSet>

    <resourceSet name="ProgressbarJs" type="js">
      <resource path="~/Scripts/Jquery/jquery.progressbar.js" />
    </resourceSet>

    <resourceSet name="TabJs" type="js">
      <resource path="~/Scripts/Tab.js" />
    </resourceSet>

    <resourceSet name="BlueTabCss" type="css">
      <resource path="~/Skins/Blue/Styles/toggle.css" />
      <resource path="~/Skins/Blue/Styles/tabs.css" />
    </resourceSet>

    <resourceSet name="DefaultTabCss" type="css">
      <resource path="~/Skins/Default/Styles/toggle.css" />
      <resource path="~/Skins/Default/Styles/tabs.css" />
    </resourceSet>
    
    <resourceSet name="EFormJs" type="js">
      <resource path="~/Scripts/jquery.ui/jquery.ui.core.js"/>
      <resource path="~/Scripts/jquery.ui/jquery.ui.widget.js"/>
      <resource path="~/Scripts/jquery.ui/jquery.ui.button.js"/>
      <resource path="~/Scripts/jquery.ui/jquery.ui.mouse.js"/>
      <resource path="~/Scripts/jquery.ui/jquery.ui.draggable.js"/>
      <resource path="~/Scripts/jquery.ui/jquery.ui.position.js"/>
      <resource path="~/Scripts/jquery.ui/jquery.ui.resizable.js"/>
      <resource path="~/Scripts/jquery.ui/jquery.ui.dialog.js" />
      <resource path="~/Scripts/Dialog.js" />
    </resourceSet>

    <resourceSet name="DefaultEFormCss" type="css">
      <resource path="~/Skins/Default/Styles/toggle.css" />
      <resource path="~/Skins/Default/Styles/tabs.css" />
    </resourceSet>

    <resourceSet name="BlueEFormCss" type="css">
      <resource path="~/Skins/Default/Styles/toggle.css" />
      <resource path="~/Skins/Default/Styles/tabs.css" />
    </resourceSet>
    
    <resourceSet name="CheckPlug_inJs" type="js">
      <resource path="~/Plugins/eCloud/Scripts/CheckPlug_in.js" />
    </resourceSet>

    <resourceSet name="LoaddingBarJs" type="js">
      <resource path="~/Scripts/JQuery.LoaddingBar/LoaddingBar.js" />
    </resourceSet>

    <resourceSet name="CorrectPngJs" type="js">
      <resource path="~/Scripts/correctpng.js" />
    </resourceSet>
  </resourceSets>
</combres>
